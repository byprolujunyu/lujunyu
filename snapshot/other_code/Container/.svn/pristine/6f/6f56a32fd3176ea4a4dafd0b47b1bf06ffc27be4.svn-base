<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/topbar"
            layout="@layout/topbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <android.support.v4.widget.SwipeRefreshLayout
            android:id="@+id/refresh"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/background">


            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/focus"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:padding="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_littile"
                            android:text="@string/identification_usertype"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <RadioButton
                            android:id="@+id/seller"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_margin="5dp"
                            android:button="@drawable/checkbox_circle"
                            android:checked="true"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_usertype2"
                            android:textColor="@color/grey_deep"
                            android:textSize="@dimen/text_2"/>


                        <RadioButton
                            android:id="@+id/buyer"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_margin="5dp"
                            android:button="@drawable/checkbox_circle"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_littile"
                            android:text="@string/identification_usertype1"
                            android:textColor="@color/grey_deep"
                            android:textSize="@dimen/text_2"/>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/margin_littile"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_littile"
                            android:text="@string/identification_identitytype"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <RadioButton
                            android:id="@+id/personal"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_margin="5dp"
                            android:button="@drawable/checkbox_circle"
                            android:checked="true"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_littile"
                            android:text="@string/identification_identitytype1"
                            android:textColor="@color/grey_deep"
                            android:textSize="@dimen/text_2"/>

                        <RadioButton
                            android:id="@+id/company"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_margin="5dp"
                            android:button="@drawable/checkbox_circle"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_identitytype2"
                            android:textColor="@color/grey_deep"
                            android:textSize="@dimen/text_2"/>

                    </LinearLayout>


                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/crack_height"
                        android:background="@color/background"
                        android:layout_marginTop="@dimen/margin_littile"/>

                    <TextView
                        android:id="@+id/tv_info"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="20dp"
                        android:text="用户信息"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_3"
                        android:layout_marginTop="@dimen/margin_littile"/>

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border_two"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:id="@+id/tv_userId"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_user_id"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <TextView
                            android:id="@+id/id"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:gravity="right"
                            android:text=""
                            android:hint="ID由系统自动分配"
                            android:textColor="@color/grey_deep"
                            android:textSize="@dimen/text_3"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/name"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_user_name"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:id="@+id/name_text"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/block_height"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right|center"
                            android:hint="@string/identification_user_name_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/phone"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_user_phone"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:id="@+id/phone_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_user_phone_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"
                            android:inputType="number"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/address"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_user_address"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_user_address_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/weixin"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_weixin"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:id="@+id/weixin_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_weixin_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/qq"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="推荐人"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:id="@+id/qq_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="请输入推荐人手机号码"
                            android:singleLine="true"
                            android:maxLength="11"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"
                            android:inputType="number"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/company_name"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_company_name"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:id="@+id/company_name_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_company_name_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/company_legal_entity"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_company_legalperson"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_company_legalperson_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/company_phone"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_company_phone"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:id="@+id/company_phone_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_company_phone_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"
                            android:inputType="number"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/company_address"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_company_address"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:id="@+id/company_address_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_company_address_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/company_person_in_charge"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_company_person"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:id="@+id/company_person_in_charge_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_company_person_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/bank_id_name"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_bank_name"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <EditText
                            android:id="@+id/bank_name_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_bank_name_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/bank"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_bank"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                      <!--  <EditText
                            android:id="@+id/bank_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_bank_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>-->

                        <TextView
                            android:id="@+id/bank_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:gravity="right"
                            android:text=""
                            android:hint="@string/change_personal_info_bank_hint_two"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"/>

                        <ImageView
                            android:layout_width="10dp"
                            android:layout_height="5dp"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:src="@drawable/down"/>

                    </LinearLayout>



                    <LinearLayout
                        android:id="@+id/bank_id"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/block_height"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/margin_littile"
                        android:paddingRight="@dimen/margin_littile">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/identification_bank_id"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <com.bm.container.view.XEditText
                            android:id="@+id/bank_id_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_weight="1"
                            android:maxLength="23"
                            android:background="@color/transparent"
                            android:gravity="right"
                            android:hint="@string/identification_bank_id_hint"
                            android:singleLine="true"
                            android:textColor="@color/grey_light"
                            android:textSize="@dimen/text_3"
                            android:inputType="number"/>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:orientation="vertical">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/crack_height"
                            android:background="@color/background"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="20dp"
                            android:text="地区"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"
                            android:layout_marginTop="@dimen/margin_littile"/>


                        <LinearLayout
                            android:id="@+id/country"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/block_height"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_marginRight="@dimen/margin_littile"
                            android:layout_marginTop="@dimen/margin_littile"
                            android:background="@drawable/round_corner_grey_border"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:paddingLeft="@dimen/margin_littile"
                            android:paddingRight="@dimen/margin_littile">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/identification_country"
                                android:textColor="@color/black"
                                android:textSize="@dimen/text_3"/>

                            <TextView
                                android:id="@+id/countryText"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/margin_littile"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:text="中国"
                                android:textColor="@color/grey_light"
                                android:textSize="@dimen/text_3"/>

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="5dp"
                                android:layout_marginLeft="@dimen/margin_littile"
                                android:src="@drawable/down"/>


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/city"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/block_height"
                            android:layout_marginLeft="@dimen/margin_littile"
                            android:layout_marginRight="@dimen/margin_littile"
                            android:layout_marginTop="@dimen/margin_littile"
                            android:background="@drawable/round_corner_grey_border"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:paddingLeft="@dimen/margin_littile"
                            android:paddingRight="@dimen/margin_littile">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/identification_city"
                                android:textColor="@color/black"
                                android:textSize="@dimen/text_3"/>

                            <TextView
                                android:id="@+id/city_text"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/margin_littile"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:text="@string/identification_city_choose"
                                android:textColor="@color/grey_light"
                                android:textSize="@dimen/text_3"/>

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="5dp"
                                android:layout_marginLeft="@dimen/margin_littile"
                                android:src="@drawable/down"/>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/chooose_city"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@drawable/round_corner_grey_border"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:padding="@dimen/margin_littile">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/identification_maincity"
                                android:textColor="@color/black"
                                android:textSize="@dimen/text_3"/>

                            <TextView
                                android:id="@+id/delete"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/margin_littile"
                                android:background="@drawable/round_corner_blue_border"
                                android:paddingBottom="5dp"
                                android:paddingLeft="@dimen/margin_littile"
                                android:paddingRight="@dimen/margin_littile"
                                android:paddingTop="5dp"
                                android:text="@string/identification_delete"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/text_3"/>

                            <TextView
                                android:id="@+id/add"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/margin_littile"
                                android:background="@drawable/round_corner_blue_border"
                                android:paddingBottom="5dp"
                                android:paddingLeft="@dimen/margin_littile"
                                android:paddingRight="@dimen/margin_littile"
                                android:paddingTop="5dp"
                                android:text="@string/identification_add"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/text_3"/>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="10dp"
                            android:background="@color/background"/>

                        <com.bm.container.view.ExtraGridView
                            android:id="@+id/gridview"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:numColumns="3"
                            android:verticalSpacing="10dp"/>

                    </LinearLayout>

                    <TextView
                        android:id="@+id/choose_city_tip"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_littile"
                        android:layout_marginRight="@dimen/margin_littile"
                        android:layout_marginTop="5dp"
                        android:text="@string/identification_tip1"
                        android:textColor="@color/grey_deep"
                        android:textSize="@dimen/text_1"/>

                    <View
                        android:id="@+id/viewllBusinessLicense"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/crack_height"
                        android:background="@color/background"
                        android:layout_marginTop="@dimen/margin_littile"/>

                    <LinearLayout
                        android:id="@+id/llBusinessLicense"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:padding="@dimen/margin_littile"
                        android:background="@color/white">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="企业法人资质证照（点击图片上传）"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <ImageView
                            android:id="@+id/ivAddBusinessLicense"
                            android:layout_width="match_parent"
                            android:layout_height="201dp"
                            android:padding="2dp"
                            android:layout_marginTop="5dp"
                            android:src="@drawable/pic_two"
                            android:background="@drawable/round_corner_grey_border"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/llIdentity"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:padding="@dimen/margin_littile"
                        android:background="@color/white">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="半身照（需手持身份证正面拍摄，点击图片上传）"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <ImageView
                            android:id="@+id/ivAddPositive"
                            android:layout_width="match_parent"
                            android:layout_height="201dp"
                            android:layout_marginTop="5dp"
                            android:padding="2dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/banshenzhao"
                            android:background="@drawable/round_corner_grey_border"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:text="身份证正面（点击图片上传）"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_3"/>

                        <ImageView
                            android:id="@+id/ivAddTheOtherSide"
                            android:layout_width="match_parent"
                            android:layout_height="201dp"
                            android:layout_marginTop="5dp"
                            android:padding="2dp"
                            android:src="@drawable/pic"
                            android:background="@drawable/round_corner_grey_border"/>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_littile"
                        android:background="@color/background"
                        android:orientation="vertical"
                        android:padding="@dimen/margin_littile">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:text="@string/identification_tip2"
                            android:textColor="@color/grey_deep"
                            android:textSize="@dimen/text_1"/>

                        <Button
                            android:id="@+id/register"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/block_height"
                            android:background="@drawable/round_corner_blue_gb"
                            android:text="@string/identification_register"
                            android:textColor="@color/white"
                            android:textSize="@dimen/text_5"/>
                    </LinearLayout>

                </LinearLayout>

            </ScrollView>


        </android.support.v4.widget.SwipeRefreshLayout>

    </LinearLayout>

</layout>
